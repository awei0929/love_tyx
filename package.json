{
  "name": "admin-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev",
    "build:test": "vite build --mode test",
    "build:gray": "vite build --mode gray",
    "build:prod": "vite build --mode prod",
    "commit": "git-cz",
    "preview": "vite preview",
    "eslint": "eslint src/ --ext .ts,.tsx --fix --cache",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix --cache",
    "lint": "npm run eslint && npm run stylelint",
    "lint:lint-staged": "lint-staged",
    "fix": "npm run fix:es && npm run fix:style && npm run fix:prettier || exit 1",
    "fix:es": "eslint --ext \".js,.ts,.tsx,.jsx\" --ignore-path .eslintignore ./ --cache --fix",
    "fix:style": "stylelint \"**/*.{css,less}\" --cache --fix ",
    "fix:prettier": "prettier --write \"**/*.{ts,js,tsx,jsx,css,less,json,yml,yaml,md,mdx}\" ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.40.2",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@bhb-frontend/admin-components": "^0.0.0-bata.1",
    "@bhb-frontend/service-types": "0.0.1-beta.4",
    "@bhb-frontend/utils": "^0.1.11",
    "@loadable/component": "^5.15.2",
    "@sentry/react": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "ahooks": "^3.7.2",
    "ali-oss": "^6.17.1",
    "axios": "^1.1.3",
    "eslint-config-airbnb": "^19.0.4",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/ali-oss": "^6.16.2",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/react-redux": "^7.1.24",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-legacy": "^2.3.0",
    "@vitejs/plugin-react": "^2.2.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "less": "^4.1.2",
    "less-loader": "7.3.0",
    "lint-staged": "^11.2.6",
    "postcss-less": "^4.0.1",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-less": "^1.0.5",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-checker": "^0.5.0",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-imp": "^2.3.0",
    "vite-plugin-stylelint": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
